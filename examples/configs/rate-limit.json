{
  "name": "Rate Limiting Simulation",
  "description": "Simula rate limiting de APIs como Twitter o GitHub",
  "target": "https://api.github.com",
  "enabled": true,
  "rules": {
    "inject_failure_rate": 0.20,
    "error_code": 429,
    "error_body": "{\"message\": \"API rate limit exceeded\", \"documentation_url\": \"https://docs.github.com/rest/overview/resources-in-the-rest-api#rate-limiting\"}"
  }
}
